{
  "name": "demo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "codegen": "graphql-let",
    "dev": "npm run codegen && next dev",
    "build": "npm run codegen && next build",
    "start": "npm run codegen && next start",
    "lint:code": "eslint \"src/**/*.{ts,tsx,js,jsx}\" --fix",
    "lint:styles": "stylelint \"src/**/*.{css,scss,sass}\" --fix",
    "prettier": "prettier --write \"src/**/*.{css,scss,sass,ts,tsx,js,jsx,md,mdx,json}\"",
    "storybook": "start-storybook -s ./public -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@apollo/client": "^3.3.15",
    "@graphql-tools/load-files": "^6.3.2",
    "next": "10.1.3",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.13.15",
    "@graphql-codegen/cli": "^1.21.3",
    "@graphql-codegen/fragment-matcher": "^2.0.1",
    "@graphql-codegen/introspection": "^1.18.1",
    "@graphql-codegen/plugin-helpers": "^1.18.4",
    "@graphql-codegen/typescript": "^1.21.1",
    "@graphql-codegen/typescript-operations": "^1.17.15",
    "@graphql-codegen/typescript-react-apollo": "^2.2.3",
    "@storybook/addon-a11y": "^6.2.7",
    "@storybook/addon-actions": "^6.2.7",
    "@storybook/addon-essentials": "^6.2.7",
    "@storybook/addon-links": "^6.2.7",
    "@storybook/addon-viewport": "^6.2.7",
    "@storybook/react": "^6.2.8",
    "@svgr/webpack": "^5.5.0",
    "@types/node": "^14.14.39",
    "@types/react": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "babel-loader": "^8.2.2",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-mdx": "^1.12.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "graphql": "^15.5.0",
    "graphql-let": "^0.17.1",
    "husky": "^6.0.0",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "stylelint": "^13.12.0",
    "stylelint-prettier": "^1.2.0",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "typescript": "^4.2.4",
    "webpack": "^4.46.0",
    "yaml-loader": "^0.6.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsc --noEmit --skipLibCheck && lint-staged"
    }
  },
  "lint-staged": {
    "./src/**/*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --cache --fix --max-warnings 0"
    ],
    "./src/**/*.{css,scss}": [
      "prettier --write",
      "stylelint --fix --max-warnings 0"
    ],
    "./src/**/*.{md,mdx,json}": [
      "prettier --write"
    ]
  }
}
